{
  "window.zoomLevel": 1,
  "material-icon-theme.hidesExplorerArrows": true,
  "workbench.colorTheme": "Visual Studio 2017 Dark - C++",
  "workbench.iconTheme": "vs-seti",
  "workbench.statusBar.visible": true,
  "workbench.editor.showTabs": "multiple",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": true,
  "window.title": "STFU AND FOCUS",
  "window.titleBarStyle": "custom",
  "window.dialogStyle": "custom",
  "window.openFilesInNewWindow": "off",
  "workbench.colorCustomizations": {
    // "editorSuggestWidget.selectedBackground": "#231739",
    // "sideBar.background": "#191521",
    // "list.activeSelectionBackground": "#231739",
    // "list.inactiveSelectionBackground": "#231739",
    // "list.focusBackground": "#231739",
    // "list.hoverBackground": "#231739",
    "terminalCursor.foreground": "#6dff5d",
    "statusBar.background": "#1A1A1A",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238"
  },

  "editor.accessibilitySupport": "off",
  "editor.autoIndent": "none",
  "editor.bracketPairColorization.enabled": true,
  "editor.emptySelectionClipboard": true,
  "editor.comments.insertSpace": true,
  "editor.codeLens": false,
  "editor.minimap.enabled": false,
  "editor.guides.indentation": false,
  "editor.renderWhitespace": "all",
  "editor.renderLineHighlight": "line",
  "editor.matchBrackets": "always",
  "editor.lightbulb.enabled": "off",
  "editor.hover.enabled": true,
  "editor.showFoldingControls": "mouseover",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.overviewRulerBorder": true,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.tabSize": 4,
  "editor.detectIndentation": true,
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 16,
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.5,
  "breadcrumbs.enabled": false,
  "explorer.confirmDragAndDrop": true,
  "explorer.confirmDelete": true,
  "explorer.compactFolders": false,
  "git.decorations.enabled": true,
  "files.insertFinalNewline": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 10000,
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.lineHeight": 1.5,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "monospace",
  "files.associations": {
    "iostream": "cpp",
    "deque": "cpp",
    "format": "cpp",
    "list": "cpp",
    "vector": "cpp",
    "xstring": "cpp",
    "array": "cpp",
    "chrono": "cpp",
    "forward_list": "cpp",
    "string": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "string_view": "cpp",
    "initializer_list": "cpp",
    "ranges": "cpp",
    "span": "cpp",
    "stacktrace": "cpp",
    "regex": "cpp",
    "valarray": "cpp"
  },
  "C_Cpp.errorSquiggles": "disabled",
  "cmake.configureOnOpen": false,
  "workbench.activityBar.location": "top",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "workbench.editor.editorActionsLocation": "hidden",
  "cmake.showOptionsMovedNotification": false,
  "workbench.editor.enablePreview": false,
  "window.customTitleBarVisibility": "auto",
}
