#include <iostream>
#include <string>
#include <vector>
using u8 = char;
using i32 = int;
using u32 = unsigned long;
using i64 = long long;
using u64 = unsigned long long;

int main() {
    std::ios_base::sync_with_stdio(false);
    std::cin.tie(nullptr);
    std::cout.tie(nullptr);

    int T;
    std::cin >> T;
    while (T--) {
	int n, m;
	std::cin >> n >> m;
	std::vector<std::vector<char>> vec(n, std::vector<char>());
	int cg = 0;
	int cb = 0;
	for (int i = 0; i < n; i++) {
	    for (int j = 0; j < m; j++) {
		char c;
		std::cin>>c;
		if(c=='B'){
		cb++;
		}
	    }
	}
    }
    return 0;
}
